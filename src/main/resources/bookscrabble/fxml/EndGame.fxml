<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="600.0" spacing="10" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="bookscrabble.client.view.GameWindowController">
    <Text fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="Thank you for playing!" wrappingWidth="293.8201308250427">
        <font>
            <Font name="Berlin Sans FB" size="29.0" />
        </font>
    </Text>
    <ImageView fitHeight="78.0" fitWidth="106.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@../pictures/winnerCup.jpg" />
        </image>
    </ImageView>
   <Text fx:id="winnerText" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="293.8201308250427">
      <font>
         <Font name="Berlin Sans FB" size="29.0" />
      </font>
   </Text>
    <Button fx:id="mainMenu" onAction="#mainMenuButtonClicked" prefHeight="40.0" prefWidth="110.0" text="Main Menu">
        <font>
            <Font name="Berlin Sans FB" size="18.0" />
        </font></Button>
    <Button fx:id="exit" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="33.0" prefWidth="71.0" text="Exit">
        <font>
            <Font name="Berlin Sans FB" size="19.0" />
        </font>
    </Button>
</VBox>
